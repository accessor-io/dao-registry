      "type": "object",
      "properties": {
        "metric": {
          "type": "string"
        },
        "currentValue": {
          "type": "number"
        },
        "previousValue": {
          "type": "number"
        },
        "change": {
          "type": "number"
        },
        "changePercentage": {
          "type": "number"
        },
