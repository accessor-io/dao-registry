{
  "$ref": "#/definitions/CreateDAORequest",
  "definitions": {
    "CreateDAORequest": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "symbol": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "chainId": {
          "type": "integer",
          "minimum": 1
        },
        "contractAddress": {
          "type": "string"
        },
        "tokenAddress": {
          "type": "string"
        },
        "treasuryAddress": {
          "type": "string"
        },
        "governanceAddress": {
          "type": "string"
        },
        "governanceType": {
          "type": "string"
        },
        "votingPeriod": {
          "type": "integer",
          "minimum": 1
        },
        "quorum": {
          "type": "integer",
          "minimum": 0,
          "maximum": 10000
        },
        "proposalThreshold": {
          "type": "integer",
          "minimum": 1
        },
        "socialLinks": {
          "$ref": "#/definitions/SocialLinks"
        },
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "maxItems": 10
        },
        "ensDomain": {
          "type": "string"
        }
      },
      "required": [
        "chainId",
        "contractAddress",
        "description",
        "governanceType",
        "governanceAddress",
        "name",
        "proposalThreshold",
        "quorum",
        "symbol",
        "tokenAddress",
        "treasuryAddress",
        "votingPeriod"
      ]
    },
    "GovernanceType": {
      "type": "string",
      "enum": [
        "token",
        "nft",
        "multisig",
        "hybrid"
      ]
    },
    "SocialLinks": {
      "type": "object",
      "properties": {
        "twitter": {
          "type": "string",
          "format": "uri"
        },
        "discord": {
          "type": "string",
          "format": "uri"
        },
        "telegram": {
          "type": "string",
          "format": "uri"
        },
        "github": {
          "type": "string",
          "format": "uri"
        },
        "medium": {
          "type": "string",
          "format": "uri"
        },
        "reddit": {
          "type": "string",
          "format": "uri"
        }
      }
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}