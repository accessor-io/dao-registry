{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "participationRate",
  "description": "Rate of member participation in voting over a time range.",
  "type": "object",
  "properties": {
    "key": { "const": "participationRate" },
    "label": { "type": "string", "const": "Participation Rate" },
    "unit": { "type": "string", "const": "percentage" },
    "category": { "type": "string", "const": "governance" },
    "timeRange": { "type": "string", "enum": ["1d", "7d", "30d", "90d", "365d", "all"], "default": "30d" },
    "source": {
      "type": "object",
      "properties": {
        "type": { "enum": ["derived"] },
        "onChain": { "type": "object", "properties": { "proposalEvents": { "type": "array", "items": { "type": "string" } }, "voteEvents": { "type": "array", "items": { "type": "string" } } } }
      },
      "required": ["type"]
    },
    "computation": {
      "type": "object",
      "properties": {
        "method": { "type": "string", "const": "uniqueVoters(timeRange) / totalMembers" },
        "dependencies": { "type": "array", "items": { "type": "string" }, "default": ["totalMembers"] }
      }
    },
    "examples": { "type": "array", "items": { "type": "number" } }
  },
  "required": ["key", "label", "unit", "category", "source"]
}

