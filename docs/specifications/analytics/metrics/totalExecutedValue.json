{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "totalExecutedValue",
  "description": "Total value transferred by executed proposals over a time range in reference currency.",
  "type": "object",
  "properties": {
    "key": { "const": "totalExecutedValue" },
    "label": { "type": "string", "const": "Total Executed Value" },
    "unit": { "type": "string", "const": "currency" },
    "currency": { "type": "string", "default": "ETH" },
    "category": { "type": "string", "const": "governance" },
    "timeRange": { "type": "string", "enum": ["1d", "7d", "30d", "90d", "365d", "all"], "default": "365d" },
    "source": { "type": "object", "properties": { "type": { "enum": ["derived"] } }, "required": ["type"] },
    "computation": { "type": "object", "properties": { "method": { "type": "string", "const": "sum(sum(values) for executed proposals over timeRange)" } } }
  },
  "required": ["key", "label", "unit", "category", "source"]
}

