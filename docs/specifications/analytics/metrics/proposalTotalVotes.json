{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "proposalTotalVotes",
  "description": "Total number of votes cast on a proposal.",
  "type": "object",
  "properties": {
    "key": { "const": "proposalTotalVotes" },
    "label": { "type": "string", "const": "Proposal Total Votes" },
    "unit": { "type": "string", "const": "count" },
    "category": { "type": "string", "const": "proposal" },
    "source": { "type": "object", "properties": { "type": { "enum": ["derived"] } }, "required": ["type"] },
    "computation": { "type": "object", "properties": { "method": { "type": "string", "const": "forVotes + againstVotes + abstainVotes" } } }
  },
  "required": ["key", "label", "unit", "category", "source"]
}

