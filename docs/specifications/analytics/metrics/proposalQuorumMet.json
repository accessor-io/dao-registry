{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "proposalQuorumMet",
  "description": "Whether this proposal met quorum.",
  "type": "object",
  "properties": {
    "key": { "const": "proposalQuorumMet" },
    "label": { "type": "string", "const": "Proposal Quorum Met" },
    "unit": { "type": "string", "const": "boolean" },
    "category": { "type": "string", "const": "proposal" },
    "source": { "type": "object", "properties": { "type": { "enum": ["derived"] } }, "required": ["type"] },
    "computation": { "type": "object", "properties": { "method": { "type": "string", "const": "(totalVotes >= quorum)" } } }
  },
  "required": ["key", "label", "unit", "category", "source"]
}

