{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "executionRisk",
  "description": "Risk that proposals fail to execute or are delayed (0-100).",
  "type": "object",
  "properties": {
    "key": { "const": "executionRisk" },
    "label": { "type": "string", "const": "Execution Risk" },
    "unit": { "type": "string", "const": "score" },
    "category": { "type": "string", "const": "risk" },
    "source": { "type": "object", "properties": { "type": { "enum": ["derived"] } }, "required": ["type"] },
    "computation": { "type": "object", "properties": { "method": { "type": "string", "const": "scaled(failedExecutionsRate + avg(executionDelay))" }, "dependencies": { "type": "array", "items": { "type": "string" }, "default": ["proposalExecutionEligible", "proposalTimeUntilExecution"] } } }
  },
  "required": ["key", "label", "unit", "category", "source"]
}

