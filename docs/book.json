{
  "title": "DAO Registry Documentation",
  "description": "  documentation for the DAO Registry system with multi-chain support, governance analytics, and ISO metadata standards",
  "author": "DAO Registry Team",
  "language": "en",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "expandable-chapters",
    "copy-code-button",
    "search-plus",
    "highlight",
    "sharing-plus",
    "fontsettings",
    "livereload",
    "github",
    "theme-default",
    "prism",
    "prism-themes"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/your-org/dao-registry"
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ]
    },
    "sharing": {
      "facebook": true,
      "twitter": true,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": [
        "facebook", "google", "twitter", "weibo", "instapaper"
      ]
    },
    "fontsettings": {
      "theme": "white",
      "family": "sans",
      "size": 2
    }
  },
  "variables": {
    "projectName": "DAO Registry",
    "projectVersion": "1.0.0",
    "githubUrl": "https://github.com/your-org/dao-registry",
    "docsUrl": "https://docs.dao-registry.com"
  }
} 