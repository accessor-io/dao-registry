{
  "name": "dao-registry",
  "version": "1.0.0",
  "description": "Comprehensive DAO Registry System with Multi-Chain Support",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "type-check": "tsc --noEmit",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "contract:compile": "hardhat compile",
    "contract:test": "hardhat test",
    "contract:deploy": "hardhat run scripts/deploy.js --network mainnet",
    "contract:deploy:testnet": "hardhat run scripts/deploy.js --network testnet"
  },
  "keywords": [
    "dao",
    "blockchain",
    "governance",
    "ethereum",
    "defi",
    "registry",
    "analytics"
  ],
  "author": "DAO Registry Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
