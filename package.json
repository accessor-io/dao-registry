{
  "name": "dao-registry",
  "version": "1.0.0",
  "description": "  DAO Registry system with URL encoding, reserved subdomains, ENS integration, and ISO metadata standards",
  "main": "backend/src/index.js",
  "scripts": {
    "dev": "nodemon backend/src/index.js",
    "start": "node backend/src/index.js",
    "prestart": "npm run build:schemas",
    "test": "jest",
    "lint": "eslint backend/src/",
    "build": "npm run build:schemas && npm run build:backend && npm run build:frontend",
    "build:backend": "echo 'Backend build complete'",
    "build:frontend": "cd frontend && npm run build",
    "build:schemas": "node scripts/specs/generate-json-schemas.js",
    "defs:generate": "node scripts/specs/generate-definitions-index.js",
    "defs:symbols": "node scripts/specs/generate-symbol-map.js",
    "docs:generate": "node docs/scripts/generate-docs.js",
    "docs:parse": "node docs/scripts/parse-docs.js",
    "docs:search": "node docs/scripts/search-docs.js",
    "docs:validate": "node docs/scripts/validate-docs.js",
    "docs:defs": "npm run defs:generate && npm run defs:symbols",
    "docs:all": "npm run docs:generate && npm run docs:parse && npm run docs:validate && npm run docs:defs",
    "docs:from-config": "node docs/scripts/generate-docs.js --config docs-config.json",
    "docs:parse-all": "node docs/scripts/parse-docs.js --all --index --search --report",
    "docs:complete": "node docs/scripts/generate-complete-docs.js --process-all --summary-report",
    "deploy": "node scripts/deployment/deploy.js",
    "deploy:simple": "node scripts/deployment/deploy-simple.js",
    "deploy:realtime": "node scripts/deployment/deploy-realtime-data.js",
    "deploy:subdomains": "node scripts/deployment/deploy-reserved-subdomains.js",
    "test:blockchain": "node scripts/test/test-blockchain-connection.js",
    "test:ccip": "node scripts/test/test-ccip-read.js",
    "test:schema": "node scripts/test/test-schema-management.js",
    "utils:check-contract": "node scripts/utils/check-contract.js",
    "utils:register-dao": "node scripts/utils/register-test-dao.js",
    "utils:start-demo": "node scripts/utils/start-demo.js",
    "utils:render-workflow": "node scripts/utils/render-workflow-diagram.js",
    "utils:generate-schema": "node scripts/utils/generate-schema-diagram.js",
    "utils:convert-diagram": "node scripts/utils/convert-diagram-to-image.js"
  },
  "keywords": [
    "dao",
    "blockchain",
    "governance",
    "ens",
    "ethereum",
    "metadata",
    "iso",
    "url-encoding",
    "subdomains",
    "registry"
  ],
  "author": "DAO Registry Team",
  "license": "MIT",
  "dependencies": {
    "@ensdomains/ensjs": "^2.1.0",
    "@openzeppelin/contracts": "^4.9.5",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dot": "^1.1.3",
    "dotenv": "^17.2.0",
    "env": "^0.0.2",
    "ethers": "^6.8.1",
    "express": "^4.21.2",
    "figlet": "^1.7.0",
    "hardhat": "^2.19.1",
    "helmet": "^7.1.0",
    "inquirer": "^8.2.6",
    "joi": "^17.13.3",
    "morgan": "^1.10.0",
    "ora": "^5.4.1",
    "punycode": "^2.3.1",
    "puppeteer": "^24.13.0",
    "socket": "^1.0.50",
    "socketio": "^1.0.0",
    "table": "^6.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/figlet": "^1.5.8",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "comment-parser": "^1.4.1",
    "eslint": "^8.54.0",
    "globby": "^14.0.1",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2",
    "typescript-json-schema": "^0.65.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/dao-registry.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/dao-registry/issues"
  },
  "homepage": "https://github.com/your-org/dao-registry#readme"
}
